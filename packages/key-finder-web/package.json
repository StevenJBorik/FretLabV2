{
  "name": "key-finder-web",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "rollup -c",
    "copy:html": "cp src/index.html dist/index.html",
    "copy:favicon": "cp src/favicon.ico dist/favicon.ico",
    "build:release": "yarn clean && yarn build && yarn copy:html && yarn copy:favicon",
    "serve": "serve ./dist -l 3000"
  },
  "dependencies": {
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/tasks-vision": "^0.10.4",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@surma/rollup-plugin-off-main-thread": "2.2.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "essentia.js": "^0.1.3",
    "express": "^4.18.2",
    "fft-js": "^0.0.11",
    "fretboards": "^0.5.0",
    "key-finder-wasm": "workspace:*",
    "meyda": "^5.6.2",
    "normalize.css": "8.0.1",
    "opencv-ts": "^1.3.6",
    "pitchy": "^4.0.7",
    "preact": "^10.16.0",
    "preact-router": "^4.1.1",
    "rollup": "2.79.1",
    "rollup-plugin-css-only": "4.3.0",
    "serve": "13.0.4",
    "server-static": "^2.0.5",
    "tone": "^14.7.77",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "uuid": "^9.0.0"
  },
  "packageManager": "yarn@3.5.0",
  "headers": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
  },
  "devDependencies": {
    "@yarnpkg/pnpify": "^4.0.0-rc.50"
  }
}
